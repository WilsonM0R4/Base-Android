<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.allem.alleminmotion.visacheckout.RestaurantsActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="?android:attr/actionBarSize"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@color/white"
        android:id="@+id/relativeLayout5" >

        <ImageButton
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginRight="16dp"
            android:src="@drawable/icon_menu"
            android:background="@android:color/transparent"
            android:id="@+id/imageButton9"
            android:onClick="onMenu"
            android:contentDescription="menu"
            android:layout_alignBottom="@+id/textView43"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/title_restaurants"
            android:id="@+id/textView43"
            android:layout_centerHorizontal="true"
            android:textColor="@color/magenta" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="16dp"
            android:src="@drawable/navigation__back"
            android:background="@android:color/transparent"
            android:id="@+id/imageButton10"
            android:contentDescription="menu"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:onClick="onUp" />
    </RelativeLayout>

    <WebView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:id="@+id/webView2"
        android:layout_below="@+id/relativeLayout5"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
