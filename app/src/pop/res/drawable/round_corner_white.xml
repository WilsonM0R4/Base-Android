<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="30dip" />
            <size android:height="40dp" android:width="75dp"/>

            <solid android:color="@color/HomeButtonSmall_backgroung"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="30dip" />
            <size android:height="40dp" android:width="75dp"/>
            <!--<stroke android:width="10dp" android:color="#5e7974" />-->
            <solid android:color="@color/HomeButtonSmall_backgroung"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle" >
        <corners android:radius="30dip"/>
            <size android:height="40dp" android:width="75dp"/>
            <solid android:color="@color/HomeButtonSmall_backgroung" />
            <stroke android:width="-4dp" android:color="@color/HomeButtonSmall_backgroung" />


        </shape>
    </item>
</selector>

